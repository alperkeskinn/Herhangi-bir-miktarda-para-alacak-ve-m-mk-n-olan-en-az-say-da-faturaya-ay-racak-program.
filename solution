  public static int[] GiveChange(int a)
  { 
    int [] sum = new int[6];
    
    sum[5] = a / 100;
    a = a%100;
     sum[4] = a /50;
    a = a%50;
     sum[3] = a /20 ;
    a = a%20;
     sum[2] = a /10;
    a = a%10;
     sum[1] = a /5;
    a = a%5;
     sum[0] = a /1 ;
      
    return sum;
  }
